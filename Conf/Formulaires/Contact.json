{
	"Username": {
		"name": "name",
		"options": {
			"type": "text",
			"label": "name",
			"translate": true,
			"class": "input",
			"placeholder": "Votre nom",
			"required": true,
			"value": true,
			"constraints":{
				"NotBlank": true,
				"Type": "string",
				"Regex": {
					"pattern": "/^([0-9A-Za-z]+$)/",
					"match": true
				},
				"Length":{
					"min": 5,
					"max": 16
				}
			}
		}
	},
	"Email": {
		"name": "email",
		"options": {
			"type": "email",
			"label": "email",
			"translate": true,
			"class": "input",
			"placeholder": "jean@exemple.fr",
			"required": true,
			"value": true,
			"constraints":{
				"NotBlank": true,
				"Type": "string",
				"Email": true
			}
		}
	}
}

